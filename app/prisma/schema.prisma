
generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  OTHER
}

model users {
  id              String    @id @default(uuid())
  email           String    @unique
  name            String?
  image           String?
  hashed_password String?
  role            Role      @default(OTHER)
  google_id       String?   @unique

  utm_source      String?
  created_at      DateTime  @default(now())
  updated_at      DateTime  @updatedAt
}
