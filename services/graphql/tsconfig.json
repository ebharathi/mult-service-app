{  "compilerOptions":
 {
  "target": "ES2020",
  "module": "CommonJS",
  "moduleResolution": "node",
  "outDir": "dist",
  "rootDir": ".",
  "strict": true,
  "skipLibCheck": true,
  "types": ["node"],
  "esModuleInterop": true,
  "jsx": "react-jsx",
  "jsxImportSource": "hono/jsx"
},
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
